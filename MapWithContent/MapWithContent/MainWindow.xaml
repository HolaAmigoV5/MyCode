<Window x:Class="MapWithContent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MapWithContent"
        mc:Ignorable="d"
        Title="MainWindow" Height="1080" Width="1920" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <local:I3DMapView x:Name="mapView" Panel.ZIndex="1">
            <Button Content="{Binding Name}" Width="100" Height="50" Background="Red" Command="{Binding BtnCom}"/>
        </local:I3DMapView>
    </Grid>
</Window>
