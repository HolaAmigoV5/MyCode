                                             VerticalAlignment="Center" HorizontalAlignment="Center"/>
            <lottieSharp:LottieAnimationView DefaultCacheStrategy="None" FileName="Assets/loading...json" AutoPlay="True"
                                             VerticalAlignment="Center" HorizontalAlignment="Center"/>
            <lottieSharp:LottieAnimationView DefaultCacheStrategy="None" FileName="Assets/lottiefiles.com - Star Wars Rey.json" AutoPlay="True"
                                             VerticalAlignment="Center" HorizontalAlignment="Center"/>
            <lottieSharp:LottieAnimationView DefaultCacheStrategy="None" FileName="Assets/Spider Loader.json" AutoPlay="True"
                                             VerticalAlignment="Center" HorizontalAlignment="Center"/>
            <lottieSharp:LottieAnimationView DefaultCacheStrategy="None" FileName="Assets/TwitterHeart.json" AutoPlay="True"
                                             VerticalAlignment="Center" HorizontalAlignment="Center"/>-->
